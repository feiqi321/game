
@charset "UTF-8";
#thirdPage {
  width: 100vw;
  height: 100vh;
  overflow: hidden;
}
#thirdPage .showImg {
  position: absolute;
  width: auto;
  height: auto;
  z-index: 5;
  width: 180rpx;
  height: 180rpx;
}
#thirdPage #dropGroup {
  position: absolute;
  z-index: 10;
  display: inline-block;
}
#thirdPage #dropGroup #dropImg {
  width: 180rpx;
  height: 180rpx;
}
#thirdPage #dropGroup .dropTools {
  position: absolute;
  top: 20rpx;
  left: 100%;
}
#thirdPage #dropGroup .dropTools ._img {
  width: 50rpx;
  height: 50rpx;
}
#thirdPage.main {
  background: #53a999 url(http://img.isxcxbackend1.cn/背景4.jpg) center top no-repeat;
  background-size: 100% auto;
  height: 100%;
  position: relative;
}
#thirdPage .top-tool {
  display: flex;
  justify-content: space-between;
  padding: 30rpx 40rpx;
}
#thirdPage .top-tool .user-info ._dt {
  width: 120rpx;
  height: 120rpx;
  float: left;
  border: 6rpx solid #000000;
  border-radius: 50%;
  box-sizing: border-box;
  background: center center no-repeat;
  background-size: contain;
  text-align: left;
}
#thirdPage .top-tool .user-info ._dd {
  margin-left: 140rpx;
}
#thirdPage .top-tool .user-info ._dd.name {
  margin-top: 10rpx;
  font-weight: bold;
  color: #000;
}
#thirdPage .top-tool .user-info ._dd.score {
  width: 186rpx;
  height: 56rpx;
  line-height: 48rpx;
  background: url(http://img.isxcxbackend1.cn/组90@2x.png) center center no-repeat;
  background-size: contain;
  color: #ffc63c;
  font-weight: bold;
  text-align: center;
  margin-top: 10rpx;
}
#thirdPage .top-tool .rigth-nav {
  margin-top: 50rpx;
}
#thirdPage .top-tool .rigth-nav .i-sb {
  background: url(http://img.isxcxbackend1.cn/组168.png) center center no-repeat;
  background-size: contain;
  width: 70rpx;
  height: 80rpx;
  display: inline-block;
}
#thirdPage .top-tool .rigth-nav .i-sb.active {
  background-image: url(http://img.isxcxbackend1.cn/组168.png);
}
#thirdPage .ft-box {
  background: #fff;
  border: 5rpx solid #333;
  border-radius: 16rpx;
  z-index: 1;
  box-shadow: 0 6rpx 0 #979797;
  bottom: 106rpx;
  position: absolute;
  padding: 10rpx 0;
  width: 90%;
  left: 50%;
  transform: translate(-50%, 0);
  transition: all ease 0.3s;
  transition: all ease 0.3s;
}
#thirdPage .ft-box > ._div {
  flex-wrap: wrap;
  display: flex;
}
#thirdPage .ft-box ._dl {
  width: 20%;
  box-sizing: border-box;
  padding: 0 20rpx;
}
#thirdPage .ft-box ._dl ._dt {
  border-radius: 50%;
  overflow: hidden;
  padding: 8rpx;
  background-size: 100% !important;
}
#thirdPage .ft-box ._dl ._dt::before {
  padding-top: 100%;
  content: "";
  display: block;
}
#thirdPage .ft-box ._dl ._dd {
  text-align: center;
  color: #f7c967;
  line-height: 1.6em;
  font-size: 24rpx;
}
#thirdPage .ft-box::after {
  content: "";
  position: absolute;
  width: 100%;
  height: 24rpx;
  left: -5rpx;
  bottom: -11rpx;
  z-index: -1000;
  border: 5rpx solid #333;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  border-top: 0;
}
#thirdPage .ft-nav {
  background: #fff;
  border: 5rpx solid #333;
  border-radius: 16rpx;
  line-height: 50rpx;
  height: 50rpx;
  z-index: 1;
  box-shadow: 0 6rpx 0 #979797;
  display: flex;
  bottom: 20rpx;
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translate(-50%, 0);
  justify-content: space-around;
  transition: all ease 0.3s;
}
#thirdPage .ft-nav::after {
  content: "";
  position: absolute;
  width: 100%;
  height: 24rpx;
  left: -5rpx;
  bottom: -11rpx;
  z-index: -1000;
  border: 5rpx solid #333;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;
  border-top: 0;
}
#thirdPage .ft-nav.hide {
  width: 33%;
  transform: translate(0, 0);
  left: 60%;
}
#thirdPage .ft-nav.hide ._div {
  display: none;
}
#thirdPage .ft-nav.hide ._div.rtool {
  display: block;
  width: 100%;
}
#thirdPage .ft-nav ._div {
  width: 33.33%;
  text-align: center;
  font-size: 32rpx;
  color: #666666;
  transition: all ease 0.3s;
  height: 50rpx;
  position: relative;
}
#thirdPage .ft-nav ._div::before {
  content: "";
  border: 5rpx solid #333;
  background: #959897;
  position: absolute;
  transform: rotate(45deg);
  border-left: 0;
  border-top: 0;
  width: 24rpx;
  height: 24rpx;
  top: -40rpx;
  left: 50%;
  margin-left: -14rpx;
  display: none;
}
#thirdPage .ft-nav ._div::after {
  content: "";
  border: 5rpx solid #333;
  background: #fff;
  position: absolute;
  transform: rotate(45deg);
  border-left: 0;
  border-top: 0;
  width: 24rpx;
  height: 24rpx;
  top: -48rpx;
  left: 50%;
  margin-left: -14rpx;
  display: none;
}
#thirdPage .ft-nav ._div.active {
  color: #333;
}
#thirdPage .ft-nav ._div.active::before,
#thirdPage .ft-nav ._div.active::after {
  display: block;
}
#thirdPage .ft-nav ._div .ic {
  display: inline-block;
  margin-left: 10rpx;
  width: 32rpx;
  height: 32rpx;
  background: center center no-repeat;
  background-size: contain;
  vertical-align: middle;
}
#thirdPage .ft-nav ._div .ic.zw {
  background-image: url(http://img.isxcxbackend1.cn/组172@3x.png);
}
#thirdPage .ft-nav ._div .ic.jz {
  background-image: url(http://img.isxcxbackend1.cn/组173@2x.png);
}
#thirdPage .ft-nav ._div .ic.ck {
  background-image: url(http://img.isxcxbackend1.cn/组169@3x.png);
}
#thirdPage .jzq {
  display: flex;
  flex-wrap: wrap;
  position: absolute;
  top: 344rpx;
  left: 50%;
  margin-left: -472rpx;
  width: 936rpx;
  height: 598rpx;
  transform: rotate(-20deg) skewX(50deg);
}
#thirdPage .jzq ._place.active {
  background-color: #90ff84;
}
#thirdPage .jzq ._place.noActive {
  background-color: #ff5756;
}
#thirdPage .jzq ._div {
  width: 156rpx;
  background: #5f8076;
  height: 100rpx;
  border: 1rpx dashed #e2dee8;
  box-sizing: border-box;
  opacity: 0.8;
}
#thirdPage .buydig {
  line-height: 60rpx;
  border: 7rpx solid #000;
  border-radius: 32rpx;
  position: relative;
  font-size: 24rpx;
  padding-bottom: 20rpx;
}
#thirdPage .buydig .buy-info {
  width: 30%;
  margin: 40rpx auto 80rpx;
  text-align: center;
  line-height: 1.6em;
}
#thirdPage .buydig .buy-info ._dt {
  border-radius: 50%;
  overflow: hidden;
  padding: 8rpx;
  background-size: 70% auto;
  margin-bottom: 20rpx;
}
#thirdPage .buydig .buy-info ._dt::before {
  padding-top: 100%;
  content: "";
  display: block;
}
#thirdPage .buydig .buy-info .title {
  font-size: 32rpx;
}
#thirdPage .buydig .buy-info .price {
  font-size: 24rpx;
}
#thirdPage .buydig .num {
  width: 40%;
  margin: 0 auto;
  text-align: center;
  position: relative;
  line-height: 46rpx;
  height: 50rpx;
  font-size: 32rpx;
}
#thirdPage .buydig .num .before {
  position: absolute;
  left: 0;
  top: 0;
  height: 50rpx;
  width: 50rpx;
  line-height: 46rpx;
  box-sizing: border-box;
  border: 2rpx solid #353535;
  text-align: center;
  color: #ffc658;
  border-radius: 6rpx;
}
#thirdPage .buydig .num .after {
  position: absolute;
  right: 0;
  top: 0;
  height: 50rpx;
  width: 50rpx;
  line-height: 46rpx;
  box-sizing: border-box;
  border: 2rpx solid #353535;
  text-align: center;
  color: #ffc658;
  border-radius: 6rpx;
}
#thirdPage .buydig .total {
  color: #fdc758;
  font-size: 32rpx;
  margin-top: 20rpx;
  text-align: center;
}
#thirdPage .buydig .btn-w {
  margin-top: 30rpx;
  text-align: center;
}
#thirdPage .buydig .btn-w .buy-btn {
  background: url(http://img.isxcxbackend1.cn/组177.png) center center no-repeat;
  background-size: contain;
  width: 220rpx;
  height: 90rpx;
  line-height: 86rpx;
  text-align: center;
  display: inline-block;
  color: #000000;
  font-size: 32rpx;
}
view[hidden] {
  display: none !important;
}
#real90 {
  width: 180rpx;
  height: 180rpx;
  visibility: hidden;
}